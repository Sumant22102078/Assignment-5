n = int (input ( " enter the number of rows "))

total = 0

for i in range (1, n+1) :
     for j in range ( 1 , i+1):
          total += 1
          x = total // 26
          if  total % 26 ==0 :
               print ( 'Z' , end = "")
          else :     
           print (chr ((total - x *26)+ 64), end= "" )
     print ()
